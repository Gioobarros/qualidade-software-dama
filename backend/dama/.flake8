[flake8]
max-line-length = 120
exclude = 
    venv,
    */migrations/*,
    __pycache__,
    *.pyc,
    .git,
    .github,
    manage.py
ignore = 
    E501,  #linha muito grande
    W503,  #quebra de linha antes de operador binário
    E203,  #espaço antes de :::
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    settings.py:E402